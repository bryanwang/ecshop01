<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.eb.ecshop"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="10"
        android:targetSdkVersion="15" />
    
    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    

    <application
        android:icon="@drawable/icon"
        android:label="@string/app_name"
         >
        <activity
            android:name="com.eb.ecshop.Appstart"
            android:label="@string/title_activity_main"
            android:theme="@style/Anim_style2" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.eb.ecshop.MainWindow" android:theme="@style/Anim_style2" />
        <activity android:name="com.eb.ecshop.Exit" android:theme="@style/MyDialogStyle" />
        <activity android:name="com.eb.ecshop.MainTopRightDialog" android:theme="@style/MyDialogStyleTop" />
        <activity android:name="com.eb.ecshop.Category" android:theme="@style/Anim_style2" />
        <activity android:name="com.eb.ecshop.Scan" android:theme="@style/Anim_style2" />
    </application>

</manifest>